package hanch;

import java.util.Scanner;

public class x {

	public static void main(String[] args) {
		//  구구단~~~
		
		Scanner scanner = new Scanner(System.in);
		System.out.println("최대 몇까지 보여드릴까요?");
		int t = scanner.nextInt();
	    for(int r=1; r<t; r++) {
		for(int m= 1; m < 10;m++ )
	    System.out.println(r + "X" + m + "=" + r*m); 		

	}

}
}
